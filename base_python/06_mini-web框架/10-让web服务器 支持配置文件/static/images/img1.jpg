function f_print_help
{
    F_PRINT_HELP " USAGE: $0 agent server_id BACKUP_IS_DONE
        agent  平台
    server_id  服ID
    "
}

function f_check_merge_file
{
    while read line; do
        echo $line | grep -qE "^[[:digit:]]{1,3}.[[:digit:]]{1,3}.[[:digit:]]{1,3}.[[:digit:]]{1,3} [[:alnum:]]{3} [0-9]+ [0-3]$" || { F_LOG_ERROR " 检查参数 MERGE_FILE 记录格式错误 ${line} "; exit 1;}
    done < $1
}

function f_ssh_exec
{
    ssh -A -o stricthostkeychecking=no -o GSSAPIAuthentication=no -p ${SSH_PORT} ${1} "${2}"
}

function f_ssh_check
{
    ssh -A -o stricthostkeychecking=no -o GSSAPIAuthentication=no -p ${SSH_PORT} ${1} "${2}" | grep -q "SSH_EXEC_SUCCESS"
}

function f_backup
{
    local line=($(echo ${v_merge_list[$1]} | sed 's/_/ /g'))
    local host=${line[0]}
    local agent=${line[1]}
    local server_id=${line[2]}
    local server_path=${GAME_DIR_AGENT}/${GAME_NAME}_${agent}_s${server_id}
    local server_link=${GAME_DIR_HTDOCS}/${GAME_NAME}_${agent}_s${server_id}
    local backup_path=${GAME_DIR_BACKUP}/merge/${GAME_FORK}/${a_agent}/${a_agent}_s${a_server_id}/${merge_day}

    local agent_flag=$(f_ssh_exec ${host} "sh -c 'source ${server_path}/script/agent_vars.sh; echo \${c_agent_flag:-0}'")

    [ "0"x == "${agent_flag}"x ]
    F_LOG_EXIT " 合服备份 状态[$1] ${host} ${agent} ${server_id} ${agent_flag} " ${log_file}

    f_ssh_check ${host} 'sh '${server_path}'/script/server_stop.sh '${agent}'_s'${server_id}' && echo "SSH_EXEC_SUCCESS"'
    F_LOG_EXIT " 合服备份 停服[$1] ${host} ${agent} ${server_id} ${agent_flag} " ${log_file}

    f_ssh_check ${host} 'if [ -L '${server_link}' ];then rm -f '${server_link}';fi && echo "SSH_EXEC_SUCCESS"'
    F_LOG_EXIT " 合服备份 断连接[$1] ${host} ${server_link} " ${log_file}

    mkdir -p ${backup_path}
    local backup_file=$(find ${backup_path} -maxdepth 1 -type f -name ${agent}_s${server_id}_*.tar.gz)

    if [ -z "${backup_file}" ]; then
        f_ssh_check ${host} 'sh '${server_path}'/script/merge_backup.sh '${a_agent}'_s'${a_server_id}' BACKUP_IS_DONE && echo "SSH_EXEC_SUCCESS"'
        F_LOG_EXIT " 合服备份 备份[$1] ${host} ${agent} ${server_id} ${agent_flag} " ${log_file}
    fi

    local file_count=$(ls ${backup_path}/${agent}_s${server_id}_*.tar.gz | wc -l)

    [ "1"x == "${file_count}"x ]
    F_LOG_EXIT " 合服备份 检查[$1] ${host} ${agent} ${server_id} ${file_count} " ${log_file}

    local md51=$(md5sum ${backup_path}/${agent}_s${server_id}_*.tar.gz | gawk '{print $1}')
    local md52=$(cat ${backup_path}/${agent}_s${server_id}_*.md5 | gawk '{print $1}')

    [ "${md51}"x == "${md52}"x ]
    F_LOG_EXIT " 合服备份 校验[$1] ${host} ${agent} ${server_id} ${md51} == ${md52} " ${log_file}
}

function f_merge
{
    local backup_path=${GAME_DIR_BACKUP}/merge/${GAME_FORK}/${a_agent}/${a_agent}_s${a_server_id}/${merge_day}
    local server_count=${#v_merge_list[@]}
    local backup_count=$(grep "[ OK ].*合服备份 校验.*" ${log_file} | wc -l)
    local file_count=$(ls ${backup_path}/${a_agent}_*.tar.gz | wc -l)

    [ "${server_count}"x == "${backup_count}"x -a "${server_count}"x == "${file_count}"x ]
    F_LOG_EXIT " 合服操作 检查备份 ${server_count} == ${backup_count} == ${file_count} " ${log_file}

    local all_server_list=""

    for item in ${v_merge_list[@]}; do
        item=($(echo ${item} | sed 's/_/ /g'))

        local host=${item[0]}
        local agent=${item[1]}
        local server_id=${item[2]}
        local server_path=${GAME_DIR_AGENT}/${GAME_NAME}_${agent}_s${server_id}

        local server_list=($(f_ssh_exec ${host} "sh -c 'source ${server_path}/script/agent_vars.sh; grep \"<server_id>\" \${c_path_server}/config/local.xml' | sed -e \"s#<serv#!/bin/bash
#===============================================================================
#
#          FILE: zabbix_check.sh
#
#         USAGE:
#
#   DESCRIPTION: 批量查询服状态
#
#        AUTHOR: xx
#       CREATED: 2013/10/25 17:39
#===============================================================================

function f_worker_check
{
    local list=($(echo ${v_server_list[$1]}))
    local path=${GAME_DIR_AGENT}/${list}/script

    if [ -f ${path}/agent_vars.sh ];then
        source ${path}/agent_vars.sh
    else
        F_PRINT_EXIT " 检查变量 ${path}/agent_vars.sh "
    fi

    if [ "0"x == "${c_agent_flag:-0}"x ]; then
        ## 服 stop
        local proc_num_running=$(pgrep -f "${c_path_server_no_ver}/.*/publish_${c_server_build}/EXE.*" | wc -l)
        if [ "0"x == "${proc_num_running}"x ]; then
            echo "${c_agent}_${c_server_id}" >> ${server_stop_file}
            F_PRINT_EXIT " 服关闭 ${c_agent}_${c_server_id} "
            return
        fi

        ## 服崩溃
        local agent_status=$(sh ${path}/server_status.sh)
        if [ "0"x == "${agent_status}"x ]; then
            echo "${c_agent}_${c_server_id}" >> ${server_collapse_file}
            F_PRINT_EXIT " 服崩溃 ${c_agent}_${c_server_id} "
            return
        fi

        ##数据缓存占用
        local monitor_file=$(find ${c_path_data}/server/log/dblog -type f -name cachemonitor.txt)
        local cache_use=$(tail -1 ${monitor_file} | gawk '{for (i=1;i<=NF;i++) if ($i~/cache_size/) print $i}' | gawk -F"[ :/()]" '{for (i=1;i<=NF;i++) if ($i~/[0-9]+/) {state[i]=$i}}END{print state[2]/state[3]*100}' | gawk '{print int($1+0.5)}')
        if [ ${cache_use} -gt 70 ]; then
            echo "${c_agent}_${c_server_id}:${cache_use}" >> ${server_cache_use_file}
            F_PRINT_EXIT " 缓存使用过高 ${c_agent}_${c_server_id} "
        fi

        ##跨服异常
        if [ -n "${CROSS_SPID}" -a -d ${AGENT_DIR_BIN}/cross ] && [ "${c_agent}" != "${CROSS_SPID}" ] && [ ${c_server_id_num} -le 1000 ]; then
            if grep -q "${c_agent}" ${AGENT_DIR_BIN}/cross/cross*.map;then
                local exist=1
            else
                return
            fi

            if [ "127.0.0.1"x == "${c_cross_ip}"x ]; then
                echo "${c_agent}_${c_server_id}" >> ${server_cross_down_file}
                F_PRINT_EXIT " 跨服未配置 ${c_agent}_${c_server_id} "
                return
            fi

            local pid=$(pgrep -f "${c_path_server_build}/EXEgameworld_release") ## 跨服进程id
            local tcp_num=$(grep -v "127.0.0.1" ${netstat_tmp_file} | grep "${pid}/EXE" | wc -l)
            if [ ${c_cross_open} -eq 1 -a ${tcp_num} -eq 0 ]; then
                echo "${c_agent}_${c_server_id}" >> ${server_cross_down_file}
                F_PRINT_EXIT " 跨服异常 ${c_agent}_${c_server_id} "
                return
            fi
        fi

        ##在线人数
        local rolenum_file=${GAME_DIR_AGENT}/${c_dir_agent}/data/server/log/gamelog_0/rolenum/rolenum.txt
        if [ -f ${rolenum_file} ]; then
            tail -q -n 1 ${rolenum_file} | gawk -F"[/[]" '{print $3}' >> ${server_online_file}
            if [ ! -s "${server_online_file}" ]; then
                echo "0" > ${server_online_file}
            fi
        fi

        local nginx_link=${GAME_DIR_HTDOCS}/${GAME_NAME}_${c_agent}_${c_server_id}
        if [ ! -L ${nginx_link} ]; then
            ln -s ${c_path_admin} ${nginx_link} && chown -R www:www ${nginx_link}
            F_PRINT_EXIT " 创建软连接 ${nginx_link} "
        fi

    fi
}

function f_zabbix_sender_direct
{
    local key=$1
    local file=$2
    if [ -s "${file}" ]; then
        ${DATA_DIR_ZABBIX_BASE}/bin/zabbix_sender -s ${zabbix_host} -z ${GAME_SERVER_ZABBIX} -k "${key}" -o "$(cat ${file} | xargs)" -r
    fi
}

function f_zabbix_sender
{
    local key=$1
    local file=$2
    local file_bak=${file}.bak
    if [ -f ${file} ]; then
        if [ -f ${file_bak} ];then
            local md5_file=$(md5sum ${file} | gawk '{print $1}')
            local md5_file_bak=$(md5sum ${file_bak} | gawk '{print $1}')
            if [ "${md5_file}" != "${md5_file_bak}" ]; then
                if [ -s "${file}" ]; then
                    ${DATA_DIR_ZABBIX_BASE}/bin/zabbix_sender -s ${zabbix_host} -z ${GAME_SERVER_ZABBIX} -k "${key}" -o "$(cat ${file} | xargs)" -r
                    \cp -f ${file} ${file_bak}
                else
                    ${DATA_DIR_ZABBIX_BASE}/bin/zabbix_sender -s ${zabbix_host} -z ${GAME_SERVER_ZABBIX} -k "${key}" -o "ok" -r
                    \cp -f ${file} ${file_bak}
                fi
            fi
        else
           echo "" > ${file_bak}
        fi
    fi
}

function f_batch_run
{
    local thread=$1
    local count=$2
    local worker=$3
    local fifo_file=/dev/shm/zabbix_fifo ##避免与其他脚本冲突

    [ -e ${fifo_file} ] && \rm -f ${fifo_file}
    mkfifo -m 600 ${fifo_file}
    exec 3<>${fifo_file}

    for ((t=0; t<${thread}; t++)); do
        echo > ${fifo_file}
    done

    for ((c=0; c<${count}; c++)); do
        read < ${fifo_file}
        { ${worker} ${c}; echo  > ${fifo_file}; } &
    done

    wait
    exec 3>&-
}

