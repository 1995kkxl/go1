## 参数判断
function f_delpoy_check
{
    case "${1}" in
        [0-z][0-z][0-z])
            F_PRINT_SUCCESS "确认渠道pid为 ${1}" ;;
        *)
            F_PRINT_ERROR "输入格式错误 !!!"
            f_print_help
            exit 1 ;;
    esac

    case "${2}" in
        [1-9]|[1-9][0-9]|[1-9][0-9][0-9]*)
            F_PRINT_SUCCESS "确认渠道开服ID ${2}" ;;
        *)
            F_PRINT_ERROR "输入格式错误 !!!"
            f_print_help
            exit 1 ;;
    esac
}

## 获取开服列表
function f_list_pid
{
    local deploy_temp_file=${CENTER_DIR_TMP}/deploy_todo.file

    mkdir -p $(dirname ${deploy_list})
    [ -f ${deploy_list} ] && mv ${deploy_list} ${deploy_list}.$(F_DATE)

    PS3=`echo -e "\033[1;32m 获取开服列表,请输入菜单序列号：\033[0m"`
    select menu in "今天" "明天" "自定义";do
        if [ -n "${menu}" ];then
            case ${menu} in
                "今天")
                    startday=$(date -d "0 day" "+%Y-%m-%d 00:00:00")
                    stopday=$(date -d "0 day" "+%Y-%m-%d 23:59:59")
                    break
                    ;;
                "明天")
                    startday=$(date -d "1 day" "+%Y-%m-%d 00:00:00")
                    stopday=$(date -d "1 day" "+%Y-%m-%d 23:59:59")
                    break
                    ;;
                "自定义")
                    echo -e -n "\033[1;32m请输入开始日期（xxxx-xx-xx）:\033[0m"; read start_date
                    echo -e -n "\033[1;32m请输入结束日期（xxxx-xx-xx）:\033[0m"; read stop_date

                    for date in "${start_date}" "${stop_date}";do
                        if ! echo "${date}" | grep -qE "^20[12][0-9]-[01][0-9]-[0123][0-9]$"; then
                            F_PRINT_ERROR "输入格式 ${date} 错误 !!!"
                            exit 1
                        fi
                    done

                    startday="${start_date} 00:00:00"
                    stopday="${stop_date} 23:59:59"
                    break
                    ;;
                *)
                    exit 1
                    ;;
            esac
        fi
    done

    starttime=$(date -d "${startday}" +%s )
    stoptime=$(date -d "${stopday}" +%s )

    F_PRINT_EXIT " 获取开服时间为：${startday} -> ${stopday} "

    curl -s "http://api.${GAME_URL_DOMAIN}/server/checkdeploy?starttime=${starttime}&stoptime=${stoptime}" > ${deploy_temp_file}

    if [ ! -s "${deploy_temp_file}" ]; then
        F_PRINT_ERROR " 获取开服信息失败或者开服数为 0 "
        exit 1
    fi

    sh ${CENTER_DIR_BIN}/batch_machine.sh > /dev/null
    F_PRINT_EXIT " 刷新机器状态！ "

    local machine_file=${CENTER_DIR_TMP}/list/game/host.status && [ -f "${machine_file}" ]
    F_PRINT_EXIT " 检查机器状态文件！ "

    local a_list=$(grep -v $^ ${deploy_temp_file} -c)
    local c=1000 #步长

    rm -f ${CENTER_DIR_TMP}/deploy_done.file

    while read line; do
        local p_id=$(echo "${line}" | awk '{print $1}')
        local p_on=$(echo "${line}" | awk '{print $2}')
        local p_port=$(echo "${line}" | awk '{print $(NF-1)}')
        if [ ${p_port} -ne 0 ]; then
            echo -e "\033[1;32m已经配置 ${line}\033[0m"
            echo -e "已经配置 ${line}" >> ${CENTER_DIR_TMP}/deploy_done.file
        else
            sort -n -k 2 -r ${machine_file} -o ${machine_file}
            ip_1=($(cat ${machine_file} | awk 'NR==1'))
            sed -i 1"s/${ip_1[1]}/$(( ${ip_1[1]} - ${c} ))/" ${machine_file}
            echo "${ip_1[0]} ${p_id} ${p_on}" >> ${deploy_list}
        fi
    done < ${deploy_temp_file}

    echo -e "${deploy_list} \n总开服数 ${a_list}"

    if [ -f ${deploy_list} ]; then
        cat ${deploy_list}
    fi
}

